{
  "name": "blog-without-framework",
  "version": "1.0.0",
  "description": "프레임워크 없이 내 블로그 만들기",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build:server": "tsc --project tsconfig.json --rootDir server --outDir dist/server && tsc-alias",
    "build": "npm run build:server",
    "start": "node -r tsconfig-paths/register dist/server/src/application/main.js",
    "dev": "ts-node -r tsconfig-paths/register server/src/application/main.ts"
  },
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.13.8",
    "dotenv": "^16.4.7",
    "module-alias": "^2.2.3",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.11",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.8.2"
  }
}
